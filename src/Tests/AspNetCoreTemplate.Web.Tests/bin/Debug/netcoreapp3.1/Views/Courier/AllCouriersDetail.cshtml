@using AspNetCoreTemplate.Common
@using Microsoft.AspNetCore.Mvc.Localization
@using AspNetCoreTemplate.Web.Resources
@inject LocService SharedLocalizer
@inject IViewLocalizer Localizer
@model AspNetCoreTemplate.Web.ViewModels.Couriers.CourierAllViewModel

@{
    this.ViewData["Title"] = $"Welcome to {GlobalConstants.SystemName}";
}

<div class="text-center">
    <h1 class="display-4">@this.ViewData["Title"]</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

<div class="media col-md-4">
    <div class="media-body">
        <h5 class="mt-0"><a href="">@SharedLocalizer.GetLocalizedHtmlString("Curiers")</a></h5>
    </div>
</div>

<div class="media col-md-4">
    <div class="media-body">
        <h5 class="mt-0"><a href="">@SharedLocalizer.GetLocalizedHtmlString("Cities")</a></h5>
    </div>
</div>

<div class="media col-md-4">
    <div class="media-body">
        <h5 class="mt-0"><a href="">@SharedLocalizer.GetLocalizedHtmlString("Vehichles")</a></h5>
    </div>
</div>

<a class="btn btn-danger" asp-area="" asp-controller="Users" asp-action="NavBarAdmin">@SharedLocalizer.GetLocalizedHtmlString("Back")</a>