@using Microsoft.AspNetCore.Mvc.Localization
@using AspNetCoreTemplate.Web.Resources
@inject LocService SharedLocalizer
@inject IViewLocalizer Localizer
@{
    this.ViewData["Title"] = "LocationObject Panel";
}
<ul>
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="Address" asp-action="Create">@SharedLocalizer.GetLocalizedHtmlString("Create")</a>
    </li>
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="LocationsObject" asp-action="AllByUser">@SharedLocalizer.GetLocalizedHtmlString("AllByUser")</a>
    </li>
</ul>

<a class="btn btn-danger" asp-area="" asp-controller="Address" asp-action="NavBarAddress">@SharedLocalizer.GetLocalizedHtmlString("Back")</a>