@using Microsoft.AspNetCore.Mvc.Localization
@using AspNetCoreTemplate.Web.Resources
@inject LocService SharedLocalizer
@inject IViewLocalizer Localizer
@{
    this.ViewData["Title"] = "Curier Panel";
}
<ul>
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="Courier" asp-action="Create">@SharedLocalizer.GetLocalizedHtmlString("Create")</a>
    </li>
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="Courier" asp-action="All">@SharedLocalizer.GetLocalizedHtmlString("All")</a>
    </li>
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="Courier" asp-action="ByCity">@SharedLocalizer.GetLocalizedHtmlString("ByCity")</a>
    </li>
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="Courier" asp-action="ByArea">@SharedLocalizer.GetLocalizedHtmlString("ByArea")</a>
    </li>
</ul>

<a class="btn btn-danger" asp-area="" asp-controller="Users" asp-action="NavBarAdmin">@SharedLocalizer.GetLocalizedHtmlString("Back")</a>